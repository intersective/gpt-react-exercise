{
  "name": "library-delay",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "library/delay/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/library/delay",
        "main": "library/delay/src/index.ts",
        "tsConfig": "library/delay/tsconfig.lib.json",
        "assets": ["library/delay/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "library/delay/**/*.ts",
          "library/delay/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "library/delay/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
